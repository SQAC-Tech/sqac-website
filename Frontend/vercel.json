{
  "redirects": [
    {
      "source": "/((?!maintenance).*)",
      "has": [
        {
          "type": "host",
          "value": "sqac.space"
        }
      ],
      "destination": "/maintenance",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
